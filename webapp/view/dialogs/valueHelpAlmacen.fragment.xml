<core:FragmentDefinition xmlns="sap.m" xmlns:fb="sap.ui.comp.filterbar" xmlns:core="sap.ui.core">
	<SelectDialog
		title="Almacenes"
		items="{filtrosAlmacen>/almacen}"
		liveChange=".onAlmacenDialogSearch"
		confirm=".onAlmacenDialogClose"
		cancel=".onAlmacenDialogClose" >
