<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="handleConfirm">
		<sortItems>
			<ViewSettingsItem text="Material" key="MATNR" selected="true" />
			<ViewSettingsItem text="Descripción de Material" key="MAKTX" />
			<ViewSettingsItem text="Precio" key="PRECIO" />
			<ViewSettingsItem text="Centro" key="WERKS" />
			<ViewSettingsItem text="Almacén" key="LGORT" />
			<ViewSettingsItem text="Lote" key="CHARG" />
			<ViewSettingsItem text="Calibre" key="CALIBRE" />
			<ViewSettingsItem text="Categoría" key="CATEGORIA" />
